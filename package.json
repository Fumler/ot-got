{
  "name": "ot-got",
  "version": "0.0.0",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "eslint src/** --max-warnings=0 && ava"
  },
  "description": "Convenience wrapper for got to add opentracing spans to requests",
  "main": "dist",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/Fumler/ot-got.git",
  "author": "Fredrik Pettersen <fredrik@whg.no>",
  "license": "MIT",
  "keywords": [
    "http",
    "https",
    "get",
    "got",
    "url",
    "uri",
    "request",
    "util",
    "utility",
    "simple",
    "curl",
    "wget",
    "fetch",
    "net",
    "network",
    "electron",
    "brotli",
    "opentracing",
    "tracing",
    "instrumentation",
    "dns",
    "agent"
  ],
  "dependencies": {
    "agentkeepalive": "^4.0.2",
    "cacheable-lookup": "^0.1.0",
    "got": "^9.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "ava": "^1.2.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "opentracing": "^0.14.3",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "opentracing": "^0.14.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 110
  }
}
